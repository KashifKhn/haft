spring:
  application:
    name: {{.ArtifactId}}
{{if .HasJpa}}
  datasource:
    url: jdbc:h2:mem:{{.ArtifactId}}db
    driver-class-name: org.h2.Driver
    username: sa
    password:
  jpa:
    database-platform: org.hibernate.dialect.H2Dialect
    hibernate:
      ddl-auto: update
  h2:
    console:
      enabled: true
{{end}}
